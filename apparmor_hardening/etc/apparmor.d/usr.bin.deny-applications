# Last Modified: 
#include <tunables/global>

/usr/bin/telnet.netkit {
  #include <abstractions/base>

  /usr/bin/telnet.netkit mr,

        deny owner @{HOME}/ r,
        deny owner @{HOME}/Public/ r,
        deny owner @{HOME}/Public/* r,
        deny owner @{HOME}/Downloads/ r,
        deny owner @{HOME}/Downloads/* rw,
        deny owner @{HOME}/Music/ r,
        deny owner @{HOME}/Music/* r,
        deny owner @{HOME}/Pictures/ r,
        deny owner @{HOME}/Pictures/* rw,
        deny owner @{HOME}/Documents/ r,
        deny owner @{HOME}/Documents/* r,
        deny owner @{HOME}/Videos/ r,
        deny owner @{HOME}/Videos/* rw,
        deny owner @{HOME}/Desktop/ r,
        deny owner @{HOME}/Desktop/* r,

        deny /.suspended r,
        deny /boot/initrd.img* r,
        deny /boot/vmlinuz* r,
        deny /var/cache/fontconfig/ w,
        deny @{HOME}/.local/share/recently-used.xbel r,

        deny owner @{HOME}/.config/ rw,

        deny @{HOME}/.gnupg/** mrwkl,
        deny @{HOME}/.ssh/** mrwkl,
        deny /etc/shadow mrwkl,

        deny /usr/bin/ r,
        deny /usr/lib/ r,
        deny /usr/local/bin/ r,
        deny /usr/sbin/ r,
        deny /usr/share/ r,
        deny /tmp/ r,
        deny /proc/ r,
        deny /run/ r,
        deny /media/ r,
        deny /var/ r,

        deny /sys/**/ r,
	deny /sys/devices/**/uevent r,
	deny /sys/devices/system/**/meminfo r,
	deny /sys/devices/pci[0-9]*/**/{config,revision} r,
        deny /sys/devices/pci[0-9]*/**/{,subsystem_}{device,vendor} r,

        # Block compiled libraries & executables
	audit deny owner /**/* mx,

	deny /run/udev/data/** rwklmx,
	deny owner /**/*.py* r,
	deny owner @{HOME}/.recently-used.xbel* rw,
	deny owner @{HOME}/.local/share/recently-used.xbel* rw,

	# Temp files
        deny /tmp/xauth-* r,

}

