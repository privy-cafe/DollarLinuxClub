        deny /sys/**/ r,
	deny /sys/devices/**/uevent r,
	deny /sys/devices/system/**/meminfo r,
	deny /sys/devices/pci[0-9]*/**/{config,revision} r,
        deny /sys/devices/pci[0-9]*/**/{,subsystem_}{device,vendor} r,

        # Block compiled libraries & executables
	audit deny owner /**/* mx,

	deny /run/udev/data/** rwklmx,
	deny owner /**/*.py* r,
	deny owner @{HOME}/.recently-used.xbel* rw,
	deny owner @{HOME}/.local/share/recently-used.xbel* rw,

