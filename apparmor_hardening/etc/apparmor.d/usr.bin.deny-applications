        deny owner @{HOME}/Pictures/* rw,
        deny owner @{HOME}/Documents/ r,
        deny owner @{HOME}/Documents/* r,
        deny owner @{HOME}/Videos/ r,
        deny owner @{HOME}/Videos/* rw,
        deny owner @{HOME}/Desktop/ r,
        deny owner @{HOME}/Desktop/* r,

        deny owner @{HOME}/.config/ rw,

        deny @{HOME}/.gnupg/** mrwkl,
        deny @{HOME}/.ssh/** mrwkl,
        deny /etc/shadow mrwkl,

        deny /usr/bin/ r,
        deny /usr/local/bin/ r,
        deny /usr/sbin/ r,
        deny /usr/share/ r,
        deny /tmp/ r,
        deny /proc/ r,
        deny /run/ r,
